		"prisma": "^5.14.0",
		"prisma-kysely": "^1.8.0",
		"svelte": "^4.2.17",
		"svelte-check": "^3.8.0",
		"svelte-meta-tags": "^3.1.2",
		"svelte-sequential-preprocessor": "^2.0.1",
