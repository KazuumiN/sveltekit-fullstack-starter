		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@tailwindcss/forms": "^0.5.7",
		"@types/eslint": "^8.56.10",
		"@types/nodemailer": "^6.4.15",
		"@types/pg": "^8.11.6",
		"@types/web-push": "^3.6.3",
		"@typescript-eslint/eslint-plugin": "^7.11.0",
